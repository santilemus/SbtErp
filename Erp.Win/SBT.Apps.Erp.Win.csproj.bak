		<ProjectReference Include="..\Erp.Module\SBT.Apps.Erp.Module.csproj" />
		<ProjectReference Include="..\Facturacion\SBT.Apps.Facturacion.csproj" />
	</ItemGroup>
	<ItemGroup>
